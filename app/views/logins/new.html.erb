<h2>Log in</h2>

<%= form_with model: @user, url: logins_path do |form| %>
  <div class="form-inputs">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div class="form-inputs">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div class="form-inputs">
    <%= form.submit 'Log in' %>
  </div>
<% end %>
